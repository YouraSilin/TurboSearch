<%= form_with url: search_phones_path, method: :post, data: { controller: 'autocomplete', action: 'input->autocomplete#search' }, local: true do |form| %>
  <%= form.search_field :name_search, placeholder: "поиск", autocomplete: "off", data: { autocomplete_target: 'input' } %>
<% end %>

<ul id="autocomplete_suggestions" class="autocomplete__list" data-autocomplete-target="suggestions"></ul>